{
  "name": "cps-blog-docusaurus-v2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "tsc -p ./scripts & docusaurus start --port 3002 --host 0.0.0.0",
    "start:scripts": "tsc -p ./scripts",
    "watch:scripts": "tsc -p ./scripts --watch",
    "build": "tsc -p ./scripts & docusaurus build",
    "compress": "cps -c ./build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "tstest": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "react": "catalog:docusaurus-v2",
    "react-dom": "catalog:docusaurus-v2",
    "@ant-design/icons": "catalog:",
    "@docusaurus/core": "catalog:docusaurus-v2",
    "@docusaurus/module-type-aliases": "^2.4.0",
    "@docusaurus/plugin-ideal-image": "^2.4.0",
    "@docusaurus/preset-classic": "catalog:docusaurus-v2",
    "@docusaurus/theme-mermaid": "catalog:docusaurus-v2",
    "@mdx-js/react": "catalog:docusaurus-v2",
    "clsx": "catalog:docusaurus-v2",
    "@node-rs/jieba": "^2.0.1",
    "docusaurus-lunr-search": "3.2.0",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.10.0",
    "prism-react-renderer": "^1.3.5",
    "rc-banner-anim": "^2.4.5",
    "rc-queue-anim": "^2.0.0",
    "tailwindcss": "catalog:",
    "lodash": "catalog:",
    "typed.js": "catalog:"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.7",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/glob": "^8.0.1",
    "@types/globby": "^9.1.0",
    "@types/lodash": "^4.14.191",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "catalog:",
    "commitizen": "catalog:",
    "commitlint-config-cz": "catalog:",
    "cz-customizable": "catalog:",
    "glob": "^8.1.0",
    "globby": "^13.1.4",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "react-popper": "catalog:",
    "stylus": "catalog:",
    "stylus-loader": "catalog:",
    "typescript": "catalog:",
    "webp-converter": "^2.3.3",
    "yaml": "catalog:"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  },
  "engines": {
    "node": ">=16.14"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
