    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.3.0",
    "postcss": "^8.5.1",
    "react-popper": "^2.3.0",
    "sass-loader": "^16.0.4",
