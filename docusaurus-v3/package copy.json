{
  "name": "cps-blog-docusaurus-v3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3001 --host 0.0.0.0",
    "build": "docusaurus build ",
    "build:github-page-test": "npx cross-env PAGE_TYPE=github BUILD_TEST=true docusaurus build --out-dir blog-docusaurus-v3",
    "build:github-page": "npx cross-env PAGE_TYPE=github  docusaurus build --out-dir blog-docusaurus-v3",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "changelog1": "npx conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "changelog2": "npx conventional-changelog -p -i CHANGELOG.md -s -r 0",
    "git": "npx conventional-changelog -p -i CHANGELOG.md -s -r 0 & git add . & git cz",
    "push": "npx conventional-changelog -p -i CHANGELOG.md -s -r 0 & git add . & git cz & git push origin master"
  },
