    "clsx": "^2.1.1",
    "cz-custom": "^0.0.2",
    "docusaurus-plugin-sass": "^0.2.6",
    "gsap": "^3.12.7",
    "is-mobile": "^5.0.0",
    "lodash": "^4.17.21",
